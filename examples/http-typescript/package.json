{
  "name": "http-typescript",
  "version": "0.0.9",
  "description": "Jobber Example, Javascript Schedule",
  "main": "./dist/index.js",
  "type": "module",
  "action": {
    "runnerAsynchronous": true,
    "runnerMinCount": 1,
    "runnerMaxCount": 16,
    "runnerMaxAge": 30,
    "runnerMaxIdleAge": 5,
    "runnerMaxAgeHard": 60,
    "runnerDockerArguments": {
      "labels": [
        {
          "key": "testing-label",
          "value": "I-Am_Eithan-Spaces-Are-Banned"
        }
      ]
    },
    "runnerMode": "standard"
  },
  "triggers": [
    {
      "type": "http",
      "name": "http-trigger",
      "path": "/count",
      "method": "GET"
    }
  ],
  "scripts": {
    "build": "rimraf ./dist/* && tsc && tsc-alias -p tsconfig.json",
    "start": "node ./dist/index.js",
    "dev": "pnpm build && node ./dist/index.js"
  },
  "keywords": [],
  "author": "Jobber",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.12",
    "rimraf": "^5.0.10",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  }
}
