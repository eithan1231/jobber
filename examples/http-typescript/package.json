{
  "name": "http-typescript",
  "version": "0.0.1",
  "description": "Jobber Example, Javascript Schedule",
  "main": "./dist/index.js",
  "type": "module",
  "action": {
    "runnerAsynchronous": true,
    "runnerMinCount": 1,
    "runnerMaxCount": 64,
    "runnerMaxAge": 300,
    "runnerMaxAgeHard": 1200,
    "runnerMode": "standard"
  },
  "triggers": [
    {
      "type": "http",
      "path": [
        "/count"
      ],
      "method": [
        "GET"
      ]
    }
  ],
  "scripts": {
    "build": "rimraf ./dist/* && tsc && tsc-alias -p tsconfig.json",
    "start": "node ./dist/index.js",
    "dev": "pnpm build && node ./dist/index.js"
  },
  "keywords": [],
  "author": "Jobber",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.12",
    "rimraf": "^5.0.10",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  }
}
